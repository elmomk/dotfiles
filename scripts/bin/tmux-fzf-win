#!/usr/bin/env bash

# SESSION=$(tmux list-windows | fzf --height=~50% | cut -d: -f1)
SESSION=$(tmux list-windows -a | cut -d: -f1,2 | fzf --preview "tmux capture-pane -ep -t {}")

tmux swap-window -t "$SESSION"



